.container
  .row
    .col-md-6.col-md-offset-3
      .well.well-sm.new-form
        %h3 Contest Pages
        .list-group
        - @pages.each do |page|
          %a.list-group-item{href: page_path(page)}
            %h4.list-group-item-heading 
              = page.title
            %p.list-group-item-text
              = page.hashtag
            
